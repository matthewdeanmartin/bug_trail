    <style>
        .log-detail {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        .log-detail dt {
            font-weight: bold;
        }

        .log-detail dd {
            margin: 0 0 20px 0;
            padding: 0 0 0 20px;
        }
    </style>
    <title>{% block title %}{% endblock %} - My Webpage</title>
    {% endblock %}
</head>
<body>
    {% include "view_navbar.jinja" %}
    <div id="content">{% block content %}{% endblock %}</div>
    <div id="footer">
        {% block footer %}
        &copy; Copyright 2024 by matthewdeanmartin, MIT License.</a>.
        {% endblock %}
    </div>
